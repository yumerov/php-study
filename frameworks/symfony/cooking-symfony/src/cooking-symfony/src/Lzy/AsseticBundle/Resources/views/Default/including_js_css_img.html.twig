{% extends '::base.html.twig' %}

{% block stylesheets %}
  {% stylesheets '@LzyAsseticBundle/Resources/public/css/*' filter='cssrewrite' %}
    <link rel="stylesheet" href="{{ asset_url }}" />
  {% endstylesheets %}
{% endblock %}

{% block javascripts %}
  {% javascripts '@LzyAsseticBundle/Resources/public/js/*' %}
    <script src="{{ asset_url }}"></script>
  {% endjavascripts %}
{% endblock %}

{% block body %}
  {% image '@LzyAsseticBundle/Resources/public/images/batman.jpg' %}
    <img src="{{ asset_url }}" alt="Example" />
  {% endimage %}
{% endblock %}

